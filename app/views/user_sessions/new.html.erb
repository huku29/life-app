<h1>ログイン</h1>

<%= form_with url: login_path, local: true do |f| %>
  <div class="form-group">
    <%= f.label :email%>
    <%= f.text_field :email%>
  </div>
  <div class="form-group">
    <%= f.label :password%>
    <%= f.text_field :passsword%>
  </div>
  <div class="action">
  <%= f.submit %>
  </div>
  <% end %>



