<h1>ログイン</h1>

<%= form_with url: login_path, local: true do |f| %>
  <div class="form-group">
    <%= f.label :email%>
    <%= f.text_field :email%>
  </div>
  <div class="form-group">
    <%= f.label :password%>
    <%= f.password_field :password%>
  </div>
  <div class="action">
    <%= f.submit %>
  </div>
<% end %>
<%= link_to '未登録の方はこちら',new_user_path %>



